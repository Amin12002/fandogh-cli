@app.on_message(Filters.command(['delall',f'delall{bot_username}']))
def deleteAll(client,message):
    global apps
    for x in apps:
        try:
            apps[x].log_out()
        except:
            pass
    apps.clear()
    client.send_message(message.chat.id,'Ù‡Ù…Ù‡ Ø§Ú©Ø§Ù†Øª Ù‡Ø§ Ø¨Ù‡ Ø¬Ø² Ù†Ù†Ù‡ Ù¾Ø§Ú© Ø´Ø¯Ù†Ø¯')

